{% extends "UserBundle::layout.html.twig" %}

{% block title %}{{ parent() }} - Liste des utilisateurs{% endblock %}

{% block body %}
    <h2>Liste des utilisateurs</h2>

    <ul>
    {% for user in users %}
        <li>
            <a href="{{ user.id }}">{{ user.username }}</a>
        </li>
    {% endfor %}
    </ul>
{% endblock %}